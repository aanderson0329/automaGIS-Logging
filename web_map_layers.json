{
  "for": "WebMapLayers",
  "layer_groups": [
    {
      "group_name": "Working TEST Layers",
      "layers": [
        {
          "layer_name": "Wards",
          "data_source": "https://opendata.arcgis.com/datasets/3b5d399346244c81b97c43514613566e_0.geojson",
          "display_fields": [
              ["WARD_LUTWARD","Ward Number","text"],
              ["WARD_LUTNAME","Ward Councilperson","text"],
              ["WARD_LUTURL","Ward URL Link","link"],
              ["WARD_LUTPIC","Councilperson Image","image"]            
            ],
          "layer_style": {
              "layer_style_rules": "function(feature) {var ward = feature.getProperty(\"CLV_WARDSWARD\");var color = \"black\";if (ward == \"1\") {color = \"yellow\";} else if (ward == \"2\") {color = \"orange\";} else if (ward == \"3\") {color = \"blue\";} else if (ward == \"4\") {color = \"green\";} else if (ward == \"5\") {color = \"grey\";} else if (ward == \"6\") {color = \"brown\";}return {fillColor: color,fillOpacity: 0.5,strokeColor: color,strokeOpacity: 1.0,strokeWeight: 1,zIndex: 1};}",
              "zIndex": 5000,
              "fillColor":"green",
              "fillOpacity": 0.5,
              "strokeColor":"green",
              "strokeOpacity": 1.0,
              "strokeWeight": 8
          },
          "toggled": true
        },

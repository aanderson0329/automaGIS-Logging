{
  "for": "WebMapLayers",
  "layer_groups": [
    {
      "group_name": "Working TEST Layers",
      "layers": [
        {
          "layer_name": "Wards",
          "data_source": "https://opendata.arcgis.com/datasets/3b5d399346244c81b97c43514613566e_0.geojson",
          "id_property_field":"CLV_WARDSOBJECTID",
          "display_fields": [
              ["WARD_LUTWARD","Ward Number","text"],
              ["WARD_LUTNAME","Ward Councilperson","text"],
              ["WARD_LUTURL","Ward URL Link","link"],
              ["WARD_LUTURLPIC","Councilperson Image","link"]            
            ],
          "layer_style": {
              "layer_style_rules": {
                "function": "var ward = feature.getProperty('CLV_WARDSWARD');var color = 'black';if (ward == '1') {color = 'yellow';} else if (ward == '2') {color = 'orange';} else if (ward == '3') {color = 'blue';} else if (ward == '4') {color = 'green';} else if (ward == '5') {color = 'grey';} else if (ward == '6') {color = 'brown';}return {fillColor: color,fillOpacity: 0.5,strokeColor: color,strokeOpacity: 1.0,strokeWeight: 1,zIndex: 1};",
                "parameters": "feature"
              }             
          },
          "legend": [
              ["Ward 1","yellow"],
              ["Ward 2","orange"],
              ["Ward 3","blue"],
              ["Ward 4","green"],
              ["Ward 5","grey"],
              ["Ward 6","brown"]
            ],
          "toggled": true
        },
        {
          "layer_name": "Medical Marijuana",
          "data_source": "https://opendata.arcgis.com/datasets/892309e55f234e92a329d03374608ae3_0.geojson",
          "id_property_field":"OBJECTID",

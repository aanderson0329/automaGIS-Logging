          "id_property_field":"OBJECTID",
          "layer_style": {
              "layer_style_rules": {
                "function": "var time = feature.getProperty('TIME_LIMIT');var color = 'black';if (time == 'None') {color = 'green';} else if (time == '4HR') {color = 'orange';} else if (time == '2HR') {color = 'yellow';} else if (time == '10HR') {color = 'blue';} return {fillColor: color,fillOpacity: 0.5,strokeColor: color,strokeOpacity: 1.0,strokeWeight: 4,zIndex: 1};",
                "parameters": "feature"
              }             
          },
          "legend": [
            ["Parking Meter Zones - No Limit", {"lineColor": "green", "lineWidth": "4px"}],

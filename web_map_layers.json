        {
          "layer_name": "Wards",
          "data_source_type": "geojson",
          "data_source": "https://opendata.arcgis.com/datasets/3b5d399346244c81b97c43514613566e_0.geojson",
          "id_property_field":"CLV_WARDSOBJECTID",
          "display_fields": [
              ["WARD_LUTWARD","Ward Number","text"],
              ["WARD_LUTNAME","Ward Councilperson","text"],
              ["WARD_LUTURL","Ward URL Link","link"],
              ["WARD_LUTURLPIC","Councilperson Image","image"]            
            ],
          "layer_style": {
              "layer_style_rules": {
                "function": "var ward = feature.getProperty('CLV_WARDSWARD');var color = 'black';if (ward == '1') {color = 'yellow';} else if (ward == '2') {color = 'orange';} else if (ward == '3') {color = 'blue';} else if (ward == '4') {color = 'green';} else if (ward == '5') {color = 'grey';} else if (ward == '6') {color = 'rgb(132, 77, 45)';}return {fillColor: color,fillOpacity: 0.5,strokeColor: color,strokeOpacity: 1.0,strokeWeight: 1,zIndex: 1};",
                "parameters": "feature"
              }             
          },
          "legend": [
              ["Ward 1",{"fillColor": "rgba(255, 250, 5, 0.5)", "borderColor": "rgba(255, 250, 5, 1)", "borderWidth": "1px"}],
              ["Ward 2",{"fillColor": "rgba(255, 159, 5, 0.5)", "borderColor": "rgba(255, 159, 5, 1)", "borderWidth": "1px"}],
              ["Ward 3",{"fillColor": "rgba(5, 38, 255, 0.5)", "borderColor": "rgba(5, 38, 255, 1)", "borderWidth": "1px"}],
              ["Ward 4",{"fillColor": "rgba(3, 178, 20, 0.5)", "borderColor": "rgba(3, 178, 20, 1)", "borderWidth": "1px"}],
              ["Ward 5",{"fillColor": "rgba(170, 170, 170, 0.5)", "borderColor": "rgba(170, 170, 170, 1)", "borderWidth": "1px"}],
              ["Ward 6",{"fillColor": "rgba(132, 77, 45, 0.5)", "borderColor": "rgba(132, 77, 45, 1)", "borderWidth": "1px"}]
            ],
          "toggled": true,
          "min_zoom": "10",
          "max_zoom": "17"
        },
